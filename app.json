{
  "expo": {
    "name": "where-is-toilet",
    "slug": "where-is-toilet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bis.wit",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "해당 위치 정보는 근처 화장실 정보를 위해 이용됩니다."
      },
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.bis.wit",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
